FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform ( 0 0 0 );

boundaryField
{
    VENTRICLE
    {
        type            movingWallVelocity;
        value           uniform ( 0 0 0 );
    }
    WALL
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
    }
    INLET
    {
        type            fixedValue;
        value           uniform ( 0 0 0.01 );
    }
    OUTLET
    {
        type            zeroGradient;
    }
}


// ************************************************************************* //
